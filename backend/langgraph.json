{
  "dependencies": ["."],
  "graphs": {
    "outline-agent": "./main.py:agent"
  },
  "env": "../.env",
  "http": {
    "cors": {
      "allow_origins": ["http://localhost:3000", "http://127.0.0.1:3000"],
      "allow_credentials": true,
      "allow_methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"],
      "allow_headers": ["*"],
      "expose_headers": ["*"],
      "max_age": 3600
    }
  }
}

